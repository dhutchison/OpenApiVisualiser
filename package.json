{
  "name": "open-apivisualiser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-container": "ng serve --host $(ip -f inet addr show eth0 | grep -Po 'inet \\K[\\d.]+')",
    "build": "ng build",
    "build-deployment": "ng build --prod --output-path dist --base-href /OpenApiVisualiser/ && cp dist/index.html dist/404.html",
    "lint": "ng lint",
    "test": "ng test --codeCoverage=true",
    "test-headless": "nyc --silent ng test --watch=false --browsers=ChromeHeadless",
    "test-headless-container": "nyc --silent ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "cy:run": "cypress run --headless --spec \"cypress/integration/*_spec.js\"",
    "cy:coverage:report": "nyc report --reporter=html --temp-dir=.nyc_output --report-dir=./coverage/cypress",
    "cy:open": "cypress open",
    "e2e": "concurrently \"ng serve\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
    "test:all": "run-s -n test-headless-container e2e test:all:coverage:report",
    "test:all:coverage:report": "nyc report --reporter=lcov --report-dir=./coverage/combined/html"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@loopback/openapi-v3-types": "^1.2.1",
    "@types/js-yaml": "^3.12.0",
    "commonmark": "^0.29.0",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.2",
    "html-to-image": "^0.1.1",
    "js-yaml": "^3.13.1",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0-rc.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "^9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/commonmark": "^0.27.4",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "concurrently": "^5.3.0",
    "cypress": "^5.0.0",
    "husky": "^4.2.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-merge": "^1.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.8",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^2.0.1",
    "ngx-build-plus": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5",
    "wait-on": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
