{
  "name": "open-apivisualiser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-container": "ng serve --host $(ip -f inet addr show eth0 | grep -Po 'inet \\K[\\d.]+')",
    "build": "ng build",
    "build-deployment": "ng build --prod --output-path dist --base-href /OpenApiVisualiser/ && cp dist/index.html dist/404.html",
    "test": "ng test --codeCoverage=true",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
    "test-headless-container": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox --codeCoverage=true",
    "test-headless-coveralls": "ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true --reporters=coverage-istanbul,coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cy:open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@loopback/openapi-v3-types": "^1.2.1",
    "@types/js-yaml": "^3.12.0",
    "commonmark": "^0.29.0",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.2",
    "html-to-image": "^0.1.1",
    "js-yaml": "^3.13.1",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0-rc.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "^9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/commonmark": "^0.27.4",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "cypress": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.8",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
